---
import { getCollection } from "astro:content";

const docs = await getCollection("connect");

console.log(docs);
---

<h1 class="text-4xl font-bold">Connect developer documentation</h1>
{
  docs.map((doc) => (
    <div class="my-4">
      <h2 class="text-lg font-semibold">{doc.id}</h2>
      <p>{doc.slug}</p>
    </div>
  ))
}
